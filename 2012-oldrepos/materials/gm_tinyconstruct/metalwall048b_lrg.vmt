// envmaptint_fix
"LightmappedGeneric"
{
	"$basetexture" "gm_tinyconstruct/p2_metal_lrg"
	"$surfaceprop" "metal"
	"%noportal" 1
	"%keywords" "portal"

	"$envmap" "env_cubemap"
	"$envmapcontrast" 1
	"$envmapsaturation" 1
	"$envmaptint" "[ 0.5 0.5 0.5 ]"

	"$reflectivity" "[ .30 .30 .30 ]" //this is a artificial radiosity boost


	"LightmappedGeneric_HDR_DX9"
	{
		"$bumpmap" "gm_tinyconstruct/p2_metal_lrg_bumpmap"
		"$normalmapalphaenvmapmask" 1
		"$envmaptint" "[ 0.5 0.5 0.5 ]"
	}

	"LightmappedGeneric_DX9"
	{
		"$bumpmap" "gm_tinyconstruct/p2_metal_lrg_bumpmap"
		"$normalmapalphaenvmapmask" 1
		"$envmaptint" "[ 0.5 0.5 0.5 ]"
	}

	"LightmappedGeneric_DX8"
	{
		"$bumpmap" "gm_tinyconstruct/p2_metal_lrg_bumpmap"
		"$nodiffusebumplighting" 1
		"$normalmapalphaenvmapmask" 1
	}

	"LightmappedGeneric_NoBump_DX8"
	{
		"$basealphaenvmapmask" 1
		"$envmaptint" "[ 0.5 0.5 0.5 ]"
	}
}
